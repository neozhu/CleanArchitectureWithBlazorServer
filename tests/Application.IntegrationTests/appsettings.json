{
  "UseInMemoryDatabase": false,
  "DatabaseSettings": {
    "DBProvider": "mssql",
    "ConnectionString": "Server=(localdb)\\mssqllocaldb;Database=BlazorDashboard.Test;Trusted_Connection=True;MultipleActiveResultSets=true;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AppConfigurationSettings": {
    "ApplicationUrl": "https://architecture.blazorserver.com",
    "Version": "1.2.3",
    "App": "Blazor",
    "AppName": "Blazor Dashboard",
    "Company": "Company",
    "Copyright": "@2024 Copyright"
  },
  "AllowedHosts": "*",
  "SmtpClientOptions": {
    "Server": "",
    "Port": 25,
    "User": "",
    "Password": "",
    "UseSsl": false,
    "RequiresAuthentication": true,
    "PreferredEncoding": "",
    "UsePickupDirectory": false,
    "MailPickupDirectory": "",
    "SocketOptions": null
  },
  "PrivacySettings": {
    "LogClientIpAddresses": true,
    "LogClientAgents": true
  },
  "IdentitySettings": {
    "RequireDigit": false,
    "RequiredLength": 6,
    "MaxLength": 16,
    "RequireNonAlphanumeric": false,
    "RequireUpperCase": false,
    "RequireLowerCase": false,
    "DefaultLockoutTimeSpan": 30
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "System": "Warning",
        "System.Net.Http.HttpClient": "Warning",
        "Hangfire.BackgroundJobServer": "Error",
        "Hangfire.Server.BackgroundServerProcess": "Error",
        "Hangfire.Server.ServerHeartbeatProcess": "Error",
        "Hangfire.Processing.BackgroundExecution": "Error",
        "BlazorState.Pipeline.State.CloneStateBehavior": "Error",
        "BlazorState.Store": "Error",
        "BlazorState.Subscriptions": "Error",
        "BlazorState.Pipeline.RenderSubscriptions.RenderSubscriptionsPostProcessor": "Error"
      }
    },
    "Properties": {
      "Application": "BlazorApp"
    }
  }
}
